    <connection>PARAM_CONN</connection>
    <sql>SELECT
CASE
	WHEN LEN(SUBSTRING(nilai1, 1, CHARINDEX(';', nilai1) - 1)) &lt;&gt; 0 THEN SUBSTRING(nilai1, 1, CHARINDEX(';', nilai1) - 1)
	ELSE NULL
END sender_name,
CASE
	WHEN LEN(SUBSTRING(nilai1, CHARINDEX(';', nilai1) + 1, LEN(nilai1))) &lt;&gt; 0 THEN SUBSTRING(nilai1, CHARINDEX(';', nilai1) + 1, LEN(nilai1))
	ELSE NULL
END sender_address,
CASE
	WHEN LEN(SUBSTRING(nilai2, 1, CHARINDEX(';', nilai2) - 1)) &lt;&gt; 0 THEN SUBSTRING(nilai2, 1, CHARINDEX(';', nilai2) - 1)
	ELSE NULL
END emailto,
CASE
	WHEN LEN(SUBSTRING(nilai2, CHARINDEX(';', nilai2) + 1, LEN(nilai2))) &lt;&gt; 0 THEN SUBSTRING(nilai2, CHARINDEX(';', nilai2) + 1, LEN(nilai2))
	ELSE NULL
END emailcc,
CASE
	WHEN LEN(SUBSTRING(nilai3, 1, CHARINDEX(';', nilai3) - 1)) &lt;&gt; 0 THEN SUBSTRING(nilai3, 1, CHARINDEX(';', nilai3) - 1)
	ELSE NULL
END subject_email,
CASE
	WHEN LEN(SUBSTRING(nilai3, CHARINDEX(';', nilai3) + 1, LEN(nilai3))) &lt;&gt; 0 THEN SUBSTRING(nilai3, CHARINDEX(';', nilai3) + 1, LEN(nilai3))
	ELSE NULL
END body_email,
--CASE
	--WHEN cast(replace('${tanggal}','','0') as int) >= 1 and cast(replace('${tanggal}','','0') as int) &lt;= 31 then replace(convert(varchar,getdate()-1,23),'-','')
	--ELSE left(replace(convert(varchar,getdate()-cast(replace('${tanggal}','','0') as int),23),'-',''),6)
--END  tgl_email
'' as tgl_email
FROM myDatabase.dbo.ms_param
WHERE id = ${flag_email}</sql>
